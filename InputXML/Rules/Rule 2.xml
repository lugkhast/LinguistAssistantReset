<ruleset name = "Tagalog Rules: Rule 2">
	<comments> Rule 2, THERE CAN ONLY BE ONE D: </comments>
	<rule name="Complement Object = Focus">
		<input>
			<component name="Cl">
				<component name="NP" matcher="$1">
					<features>
						<feature name="Complement Type" value="Object" />
					</features>
					<component name="N">
						<features>
							<feature name="Participant Tracking" value="Routine" />
						</features>
					</component>
				</component>
			</component>
		</input>
		<output>
			<action name="addFeature" parentTag="$1">
				<argument>
					<feature name="Focus Type" value="Focus" />
				</argument>
			</action>
		</output>
	</rule>
	
