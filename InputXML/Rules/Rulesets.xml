<ruleset name = "Spellout Rules">
	<comments> These rules deal with blah blah blah </comments>
	<rule name="name">
		<input>
			<component name="Cl" matcher="$1">
				<features>
						<feature name="Type" value="Independent" />
				</features>
				<component name="NP" matcher="$1-1">
					<features>
						<feature name="Semantic Role" value="Agent" />
					</features>
					<component name="N" concept="pia" sense="A" matcher="$3 />
				</component>
			</component>
		</input>
		<output>
			<action target="component" type="del">
				<argument parentTag="$1" targetTag="$2" />
			</action>
			<action target="feature" type="set">
				<argument parentTag="$3" parentComponent="N" />
				<argument>
					<feature name="Semantic Role" value="Most Agent-like" />
				</argument>
			</action>
 			<action target="feature" type="specset">
				<argument parentTag="$3" parentComponent="N" />
				<argument>
					<feature name="Semantic Role" value="Most Patient-like" />
				</argument>
				<argument value="Most Agent-like" />
			</action>
		</output>
	</rule>
	<rule name="name">
		<input>
			<component name="Cl" matcher="$1">
				<features>
						<feature name="Type" value="Independent" />
				</features>
				<component name="NP" matcher="$2">
					<features>
						<feature name="Semantic Role" value="Agent" />
					</features>
					<component name="N" concept="pia" sense="A" matcher="$3 />
				</component>
			</component>
		</input>
		<output>
			<action target="const" type="add">
				<argument label="$1" />
				<argument>
					<component name="Conjunction">
						<features>
							<feature name="place" value="holder" />
						</features>
					</component>
				</argument>
			</action>
			<action target="feature" type="del">
				<argument label="$2" />
				<argument label="semantic-role" />
			</action>
		</output>
	</rule>
</ruleset>