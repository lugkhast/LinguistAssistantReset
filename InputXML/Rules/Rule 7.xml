<ruleset name = "Tagalog Rules: Rule 7">
	<comments> Rule 7, ORDER IN THE COURT >:I </comments>
	<rule name="Order = Focus, Actor, Object, Directional, VP">
		<input>
			<component name="Cl" matcher="$1">
				<component name="NP" matcher="$2">
					<features>
						<feature name="Focus Type" value="Focus" />
					</features>
				</component>
				<component name="NP" matcher="$3">
					<features>
						<feature name="Focus Type" value="undefined" />
						<feature name="Complement Type" value="Actor" />
					</features>
				</component>
				<component name="NP" matcher="$4">
					<features>
						<feature name="Focus Type" value="undefined" />
						<feature name="Complement Type" value="Object" />
					</features>
				</component>
				<component name="NP" matcher="$5">
					<features>
						<feature name="Focus Type" value="undefined" />
						<feature name="Complement Type" value="Directional" />
					</features>
				</component>
				<component name="VP" matcher="$6" />
			</component>
		</input>
		<output>
			<action name="orderSubConst" parentTag="$1" />
			<argument order="$6 $3 $4 $5 $2" />
		</output>
	</rule>

