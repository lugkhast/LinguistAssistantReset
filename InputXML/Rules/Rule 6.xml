<ruleset name = "Tagalog Rules: Rule 6">
	<comments> Rule 6, NOT THE FINAL FORM (= A =) </comments>
	<rule name="Verb Form = AF, Imperfective">
		<input>
			<component name="Cl">
				<component name="VP" matcher="$1">
					<component name="V">
						<features>
							<feature name="Focus" value="AF" />
							<feature name="Surface Aspect" value="Imperfective" />
						</features>
					</component>
				</component>
			</component>
		</input>
		<output>
			<action name="selectForm" parentTag="$1">
				<argument>
					<feature name="Focus" value="AF" />
					<feature name="Surface Aspect" value="Imperfective" />
				</argument>
			</action>
		</output>
	</rule>

